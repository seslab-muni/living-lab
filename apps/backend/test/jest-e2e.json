{
  "moduleFileExtensions": ["js", "json", "ts", "d.ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>\\src\\$1.ts"
  },
  "moduleDirectories": [
    "node_module",
    "src"
  ]
}
